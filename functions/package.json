{
  "name": "truresetx-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for TruResetX",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "seed": "ts-node src/seedSpiritualData.ts",
    "seed:all": "ts-node src/seedAllFeaturesData.ts",
    "seed:spiritual": "npm run seed"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0",
    "axios": "^1.6.0",
    "@google-cloud/functions-framework": "^3.3.0",
    "node-fetch": "^2.7.0",
    "@types/node-fetch": "^2.6.11"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.10.0",
    "firebase-functions-test": "^3.1.0",
    "ts-node": "^10.9.2"
  },
  "private": true
}

